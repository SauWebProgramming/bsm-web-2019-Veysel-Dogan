            <th>BAŞLIK</th>
            <th>ALTBAŞLIK</th>
            <th>BÖLÜM</th>
            <th>GNO</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.Deger3)
        {
            <tr>
                <th>@x.ID</th>
